number Cos(number angle1) {
    number result = 1;
    number term = 1;
    number factorial = 1;
    for (number i = 1; i <= 10; i+=1) {
        term *= -angle1 * angle1 / ((2 * i) * (2 * i - 1));
        result += term;
    }
    return result;
}

number Sin(number angle) {
    number result = angle;
    number term = angle;
    number factorial = 1;
    for (number i = 1; i <= 10; i+=1) {
        term *= -angle * angle / ((2 * i + 1) * (2 * i));
        result += term;
    }
    return result;
}


