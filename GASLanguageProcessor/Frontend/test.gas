num canvasWidth = 950;
num canvasHeight = 950;

Color white = Color{red=255, green=255, blue=255, alpha=1};
Color red = Color{red=255, green=0, blue=0, alpha=white.alpha};
Color blue = Color{red=0, green=0, blue=255, alpha=white.alpha};
Color black = Color{red=0, green=0, blue=0, alpha=white.alpha};

TypeDef ear {
    Circle circle;
    constructor ear(Circle circle, num centerX, num centerY) {
        this.circle = circle;
        this.circle.center = Point{x=centerX, y=centerY};
    }
}

ear ear1 = ear(Circle{radius=200, stroke=10, color=white}, canvasWidth/2+60, canvasHeight/2-60);

TypeDef head {
    Circle circle;
}

TypeDef eye {
    Circle circle;
}

TypeDef face {
    head head;
    ear leftEar;
    ear rightEar;
    eye leftEye;
    eye rightEye;

    constructor face(head head, ear leftEar, ear rightEar, eye leftEye, eye rightEye) {
        this.head = head;
        this.leftEar = leftEar;
        this.rightEar = rightEar;
        this.leftEye = leftEye;
        this.rightEye = rightEye;
    }
}

Point center = Point{x=canvasWidth/2, y=canvasHeight/2};
Point leftEarPos = Point{x=center.x+60, y=center.y-60};
ear leftEar = ear {
    circle = Circle{center=leftEarPos, radius=40, stroke=10, color=white}
};

Point rightEarPos = Point{x=center.x-60, y=center.y-60};
ear rightEar = ear {
    circle = Circle{center=rightEarPos, radius=40, stroke=10, color=white}
};

face face = face {
    head = head {
        circle = Circle{center=center, radius=90, stroke=10, color=white}
    },
    leftEar = leftEar,
    rightEar = rightEar,
    leftEye = eye {
        circle = Circle{center=Point{x=center.x-30, y=center.y-30}, radius=20, stroke=10, color=white, strokeColor=black}
    },
    rightEye = eye {
        circle = Circle{center=Point{x=center.x+30, y=center.y-30}, radius=20, stroke=10, color=white, strokeColor=black}
    }
};

canvas (canvasWidth, canvasHeight, white);
