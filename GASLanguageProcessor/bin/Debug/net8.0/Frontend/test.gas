num canvasWidth = 500;
num canvasHeight = 500;

color white = Color(255, 255, 255, 1);
color pink = Color(255, 192, 203, 1);

list<circle> test(list<point> nums) {
   list<circle> circles = List<circle>{};
   for (num i = 0; i < LengthOfList(nums); i++) {
        AddToList(Circle(GetFromList(i, nums), 10, 10, white, pink), circles);
   }
   return circles;
}

list<point> points = List<point>{};
for (num x = 0; x < canvasWidth+20; x += 20) {
   for (num y = 0; y < canvasHeight+20; y += 20) {
       AddToList(Point(x, y), points);
   }
}



list<circle> nus = test(points);
canvas (canvasHeight, canvasWidth, white);
